{
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/anuraghazra/github-readme-stats"
    }
  ],
   "crons": [{
    "path": "/api/cron",
    "schedule": "*/15 * * * *"
  }]
}
